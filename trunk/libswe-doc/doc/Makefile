install-dist_astrodocDATA: $(dist_astrodoc_DATA)
	@$(NORMAL_INSTALL)
	@list='$(dist_astrodoc_DATA)'; test -n "$(astrodocdir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(astrodocdir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(astrodocdir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(astrodocdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(astrodocdir)" || exit $$?; \
	done

uninstall-dist_astrodocDATA:
